          const crops = JSON.parse(jsonStr) as MarketItem[];
          
          return crops.map((crop: MarketItem) => ({
            ...crop,
            current: Math.round(crop.avg + (Math.random() * 100 - 50)),
            history: generateHistory(crop.avg)
          }));
        }
      }
    } catch (error) {
// ... keep existing code
